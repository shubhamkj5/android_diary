<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" >
		<shape>
			<solid
				android:color="#ff1d195f" />
			<padding
				android:left="10dp"
				android:top="10dp"
				android:right="10dp"
				android:bottom="10dp" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient
				android:startColor="#ff2942ae"
				android:endColor="#ff2b4680"
				android:angle="270" />
			<padding
				android:left="10dp"
				android:top="10dp"
				android:right="10dp"
				android:bottom="10dp" />
		</shape>
	</item>
</selector>